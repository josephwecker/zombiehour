<h2>Listing point_of_interests</h2>

<table>
  <tr>
    <th>District</th>
    <th>Name</th>
    <th>Stocks</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @point_of_interests.each do |point_of_interest| %>
  <tr>
    <td><%= point_of_interest.district %></td>
    <td><%= point_of_interest.name %></td>
    <td><%= point_of_interest.stocks %></td>
    <td><%= link_to 'Show', admin_city_district_point_of_interest_path(@city, @district, point_of_interest) %></td>
    <td><%= link_to 'Edit', edit_admin_city_district_point_of_interest_path(@city, @district, point_of_interest) %></td>
    <td><%= link_to 'Destroy', admin_city_district_point_of_interest_path(@city, @district, point_of_interest), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

